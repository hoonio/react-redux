{
  "name": "hoonio-root",
  "version": "2.6.0",
  "description": "Hoonio main website",
  "repository": "github.com/hoonio/root",
  "license": "MIT",
  "engines": {
    "node": ">=7.1.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "main": "dist/server.js",
  "scripts": {
    "dev": "npm-run-all --parallel open:src",
    "open:src": "NODE_ENV=develop babel-node server/debug",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter spec tools/testSetup.js \"app/tests/**/*.spec.js\"",
    "test:watch": "npm run test -- --watch",
    "prebuild": "babel-node tools/buildHtml.js",
    "build": "babel-node tools/build.js",
    "start": "babel-node server/production.js"
  },
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "6.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.22.0",
    "body-parser": "^1.16.0",
    "bootstrap": "^4.0.0-alpha.2",
    "cheerio": "^0.22.0",
    "colors": "1.1.2",
    "compression": "^1.6.1",
    "css-loader": "^0.24.0",
    "d3": "^3.5.16",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "jsonp-es6": "^1.0.0",
    "node-sass": "^4.1.1",
    "react": "^15.4.1",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.4.1",
    "react-redux": "4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "redux": "3.5.2",
    "redux-thunk": "2.0.1",
    "sass-loader": "^4.0.0",
    "serve-static": "^1.10.2",
    "style-loader": "^0.13.1",
    "toastr": "2.1.2",
    "webpack": "^1.12.14"
  },
  "devDependencies": {
    "babel-plugin-react-display-name": "2.0.0",
    "babel-polyfill": "^6.7.4",
    "babel-register": "^6.9.0",
    "browser-sync": "^2.11.2",
    "del": "^2.2.0",
    "enzyme": "^2.2.0",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-import": "1.6.1",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.3.0",
    "eslint-watch": "2.1.11",
    "eventsource-polyfill": "0.9.6",
    "expect": "^1.20.2",
    "file-loader": "0.8.5",
    "jsdom": "8.5.0",
    "mocha": "^2.5.3",
    "nock": "8.0.0",
    "npm-run-all": "^4.0.0",
    "open": "^0.0.5",
    "react-addons-test-utils": "^15.0.0",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.2.0",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-mock-store": "1.0.2",
    "sinon": "^1.17.5",
    "url-loader": "0.5.7",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  }
